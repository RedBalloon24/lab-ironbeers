<div class="beer-container">
  {{#each beers as |beer|}} 
    <div class="beer-body">
        <img class="beer-img" src="{{beer.image_url}}">
        <h5 class="beer-name">{{beer.name}}</h5>
        <p class="beer-tagline">{{beer.tagline}}</p>
        <p class="beer-description">{{beer.description}}</p>
    </div>
  {{/each}} 
</div>